{
  "family": "podplayadmin-test-dev",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "podplayadmin-web-dev",
      "image": "251278640225.dkr.ecr.eu-west-1.amazonaws.com/podplayadmin-web-dev:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "FALLBACK_LANG",
          "value": "en"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/podplayadmin-test-dev",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "taskRoleArn": "arn:aws:iam::251278640225:role/podplayadmin-test-dev-ecs-task-role",
  "executionRoleArn": "arn:aws:iam::251278640225:role/podplayadmin-test-dev-ecs-execution-role",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
